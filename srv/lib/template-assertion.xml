<saml2:Assertion xmlns:saml2="urn:oasis:names:tc:SAML:2.0:assertion"
                 xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
                 ID="{{ASSERTION_ID}}"
                 IssueInstant="{{ISSUE_INSTANT}}"
                 Version="2.0">
  <saml2:Issuer>{{ISSUER}}</saml2:Issuer>
  <ds:Signature></ds:Signature>
  <saml2:Subject>
    <saml2:NameID Format="urn:oasis:names:tc:SAML:1.1:nameid-format:emailAddress">{{USER_EMAIL}}</saml2:NameID>
    <saml2:SubjectConfirmation Method="urn:oasis:names:tc:SAML:2.0:cm:bearer">
      <saml2:SubjectConfirmationData NotOnOrAfter="{{NOT_ON_OR_AFTER}}" Recipient="{{RECIPIENT}}"/>
    </saml2:SubjectConfirmation>
  </saml2:Subject>
  <saml2:Conditions NotBefore="{{NOT_BEFORE}}" NotOnOrAfter="{{NOT_ON_OR_AFTER}}">
    <saml2:AudienceRestriction>
      <saml2:Audience>{{AUDIENCE}}</saml2:Audience>
    </saml2:AudienceRestriction>
  </saml2:Conditions>
</saml2:Assertion>
